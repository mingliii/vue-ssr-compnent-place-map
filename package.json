{
  "name": "vue-chakra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:client": "cross-env NODE_ENV=development webpack --config ./config/webpack.client.config.js --progress",
    "build:server": "cross-env NODE_ENV=development webpack --config ./config/webpack.server.config.js --progress",
    "build": "npm run build:client && npm run build:server",
    "server:dev": "cross-env NODE_ENV=development nodemon ./src/server.js",
    "server:prod": "cross-env NODE_ENV=prodction nodemon ./src/server.js"
  },
  "dependencies": {
    "@chakra-ui/vue": "^0.8.1",
    "@emotion/css": "^11.1.3",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "node-fetch": "^2.6.1",
    "vue": "^2.6.12",
    "vue-apollo": "^3.0.7",
    "vue-server-renderer": "^2.6.12",
    "vue2-google-maps": "^0.10.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "express-http-proxy": "^1.6.2",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.1",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.6.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "nodemonConfig": {
    "watch": [
      "src/server.js",
      "src/index.template.html",
      "config/setup-dev-server.js"
    ]
  }
}
